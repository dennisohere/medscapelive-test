{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "commit": "cz",
    "bump": "npx standard-version && git push && git push -o ci.skip --tags",
    "dev": "npm run createLocalEnv && cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build:local": "npm run createLocalEnv && nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "start:local": "cross-env NODE_ENV=local node server/index.js",
    "lint": "eslint --fix --ext .js,.vue,.ts --ignore-path .gitignore .",
    "test:unit": "npm run createLocalEnv && cross-env NODE_ICU_DATA=node_modules/full-icu vitest tests/unit",
    "test:unit:fix": "npm run createLocalEnv && vitest tests/unit -u",
    "test:component": "npm run createLocalEnv && vitest tests/component ",
    "test:e2e": "cd / && git clone git@gitlab.webmd.com:WebMD/professional/medscape-live/qa/medscape-live-web-tests.git && cd medscape-live-web-tests && npm ci && npm run cy:run",
    "docker:run": "docker build -t foo . && docker run -p 80:80 foo",
    "createLocalEnv": "shx cp .env.local .env"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/robots": "^4.0.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.11",
    "express": "^4.19.2",
    "lodash": "^4.17.21",
    "material-design-icons-iconfont": "^6.7.0",
    "mitt": "^3.0.1",
    "nuxt": "^3.12.3",
    "optional": "^0.1.4",
    "qs": "^6.12.3",
    "redirect-ssl": "^3.0.0",
    "swiper": "^8.4.7",
    "vue": "latest",
    "vue-awesome-swiper": "^5.0.1",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@nuxt/test-utils": "^3.14.1",
    "@nuxt/types": "^2.18.1",
    "@nuxt/typescript-build": "^3.0.2",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/fontawesome": "^1.1.2",
    "@nuxtjs/style-resources": "^1.2.2",
    "@nuxtjs/stylelint-module": "^5.2.0",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@vue/test-utils": "^2.4.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.7.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "happy-dom": "^14.12.3",
    "jsdom": "^24.1.3",
    "nodemon": "^3.1.4",
    "playwright-core": "^1.46.1",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.77.8",
    "sass-loader": "^14.2.1",
    "shx": "^0.3.4",
    "vite-plugin-vuetify": "^2.0.3",
    "vitest": "^2.1.1",
    "vuetify": "^3.6.13"
  }
}
